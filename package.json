{
    "name": "uxmid-core",
    "version": "1.1.0",
    "description": "uxmid core library.",
    "scripts": {
        "lint": "tslint src/**/*.ts",
        "build:compile": "tsc -p .",
        "build:dist": "node build/dist.js",
        "build": "npm run lint && npm run build:compile && npm run b",
        "w": "node build/build.js",
        "r": "rollup --config ./build/rollup.conf.js",
        "b": "browserify --node --standalone ts-promise ./dist/index.js > ./dist/uxmid.js",
        "mini": "uglifyjs ./dist/uxmid.js --compress --mangle --mangle-props --mangle-regex='/^_/' --reserved 'Promise' > ./dist/uxmid.min.js",
        "unit": "karma start test/unit/karma.conf.js",
        "test": "npm run unit"
    },
    "files": [
        "dist"
    ],
    "main": "dist/uxmid.js",
    "typings": "dist/uxmid.d.ts",
    "keywords": [
        "uxmid",
        "typescript"
    ],
    "license": "MIT",
    "author": "jason <jasonsoop@gmail.com>",
    "homepage": "http://www.uxmid.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/uxmid/uxmid.core.git"
    },
    "bugs": {
        "url": "https://github.com/uxmid/uxmid.core/issues"
    },
    "devDependencies": {
        "@types/chai": "^4.0.4",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.0.10",
        "chai": "^4.1.2",
        "chalk": "^2.3.0",
        "es6-promise": "^4.1.1",
        "karma": "^4.1.0",
        "karma-mocha": "^1.3.0",
        "karma-mocha-reporter": "^2.2.5",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-webpack": "^4.0.2",
        "mocha": "^6.1.4",
        "ora": "^3.4.0",
        "phantomjs-prebuilt": "^2.1.16",
        "rimraf": "^2.6.2",
        "rollup": "^1.16.2",
        "rollup-plugin-replace": "^2.0.0",
        "ts-loader": "^6.0.4",
        "tslint": "^5.18.0",
        "tslint-config-uxmid": "^1.0.0",
        "typescript": "^3.5.2",
        "uglify-js": "^3.6.0"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "dependencies": {
        "rollup-plugin-commonjs": "^10.0.0",
        "webpack": "^4.35.0",
        "webpack-cli": "^3.3.5"
    }
}
